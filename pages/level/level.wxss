/* level/level.wxss */
.level-container{
  display: flex;
  flex-wrap: wrap;
  padding: 0 50rpx;
}
.level-item-wrap{
  width: 25%;
}

.level-item {
  margin: 0 20rpx 40rpx 20rpx;
  padding-top: calc(105.505% - 40rpx);
  position: relative;
}

.level-counter, .level-time{
  display: block;
  text-align: center;
}

.level-content{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  text-align: center;
  color: #fff;
}

.level-content.lock{
  background-image: url('data:image/png;base64,');
}

.level-content.complete {
  background-image:url('data:image/png;base64,');
}

.level-counter{
  font-size: 50rpx;
  font-weight: bold;
  margin-top: 30rpx;
}

.level-time{
  font-size: 22rpx;
}

.select-level{
  width: 182rpx;
  height: 85rpx;
  margin: 20rpx auto  40rpx;
  background: url('data:image/png;base64,');
  background-size: 100%;
}